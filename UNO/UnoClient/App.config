<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_ILoginAndSignUp" />
                <binding name="WSDualHttpBinding_IGame" />
                <binding name="WSDualHttpBinding_ILobby" />
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/UNOService/Service1/one"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_ILoginAndSignUp"
                contract="proxy.ILoginAndSignUp" name="WSDualHttpBinding_ILoginAndSignUp">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/UNOService/Service1/two"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IGame"
                contract="proxy.IGame" name="WSDualHttpBinding_IGame">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/UNOService/Service1/three"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_ILobby"
                contract="proxy.ILobby" name="WSDualHttpBinding_ILobby">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>